    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.1",
    "rollup-plugin-license": "^2.2.0",
    "rollup-plugin-sizes": "^1.0.3",
